{
  "name": "gpt-context-generator",
  "displayName": "GPT Context Generator",
  "description": "Generate GPT-friendly context for js/ts files or workspaces",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Machine Learning",
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "keywords": [
    "gpt-4",
    "gpt",
    "openai",
    "generator",
    "context"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gpt-context-generator.createGPTFriendlyContext",
        "title": "Create GPT Friendly Context"
      },
      {
        "command": "gpt-context-generator.createGPTFriendlyContextForOpenFile",
        "title": "Create GPT Friendly Context for this file and its dependencies"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "ignore": "^5.2.4"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codybrom/gpt-context-generator.git"
  },
  "license": "MIT",
  "publisher": "codybrom"
}